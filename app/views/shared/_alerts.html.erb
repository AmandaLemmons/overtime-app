<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], :title => "Overtime App Notificaiton", :sticky => false) %>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], :title => "Overtime App Notificaiton", :sticky => false) %>
<% else  %>
  <%= js add_gritter(flash[:notice], :title => "Overtime App Notificaiton", :sticky => false) %>
<% end %>
